{
  "name": "Beamed",
  "description": "A web extension for Beamed that allows you to beam media between your computer and your iOS device.",
  "version": "1.0.0",
  "permissions": [
    "https://*/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "icons": {
    "16": "icons/Beamed_16.png",
    "48": "icons/Beamed_48.png",
    "128": "icons/Beamed_128.png"
  },
  "browser_action": {
    "default_title": "Beamed",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}